<launch>
    <!-- 设置地图的配置文件 -->
    <arg name="map" default="306_gmapping.yaml" />
    <!-- 运行地图服务器，并且加载设置的地图-->
    <node name="map_server" pkg="map_server" type="map_server" args="$(find agv_nav)/map/$(arg map)"/>
    <!-- 启动AMCL节点 -->
    <include file="$(find agv_nav)/launch/nav04_amcl.launch" />
</launch>
